@{

    switch (ViewBag.Section)
    {
        case "registered":
            await Html.RenderPartialAsync("_RegisterView");
            break;

        case "subscribed":
            await Html.RenderPartialAsync("_SubscribeView");
            break;
    }
}


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
